{
  "name": "jmespath.site",
  "version": "1.0.0",
  "description": "[![gitter](https://badges.gitter.im/Join%20Chat.svg)](https://gitter.im/jmespath/chat)",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:e2e",
    "build": "node scripts/build.cjs",
    "build:test": "node scripts/build.cjs --build-only",
    "build:strict": "node scripts/build.cjs --fail-on-validation-error",
    "validate": "node scripts/validate-jmespath.cjs",
    "validate:verbose": "node scripts/validate-jmespath.cjs --verbose",
    "validate:strict": "node scripts/validate-jmespath.cjs --fail-fast",
    "dev": "npm run build && npx serve docs -p 3000",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@jmespath-community/jmespath": "^1.3.0",
    "@playwright/test": "^1.48.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.8",
    "flexsearch": "^0.8.205",
    "gray-matter": "^4.0.3",
    "happy-dom": "^18.0.1",
    "marked": "^16.1.1",
    "node-html-parser": "^7.0.1",
    "serve": "^14.2.4",
    "vitest": "^3.2.4"
  }
}
